#include <stdio.h>
#include <math.h>
 
int main(){
    int a;
    scanf("%d", &a);
    for(int j=0;j<a;j++)
    {
		int n;
		scanf("%d",&n);
        int c = 0;
        for(int i = 2; i <= sqrt(n); i++)
        {
			if(n % i == 0)
			c++;
        }
    if(c == 0)
        printf("YES\n");
    else
        printf("NO\n");
    }
}
